{
  "version": 2,
  "buildCommand": "cd ../.. && pnpm build --filter=@mathquest/web",
  "installCommand": "cd ../.. && pnpm install",
  "framework": "nextjs",
  "regions": ["iad1"],
  "outputDirectory": "apps/web/.next",
  "functions": {
    "apps/web/src/app/api/**/*.ts": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/apps/web/$1"
    }
  ]
}